{
  "expo": {
    "name": "ParkEase",
    "slug": "parking",
    "version": "1.0.0",
    "scheme": "parking",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jyrwaboys.parking"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jyrwaboys.parking"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8c409e50-e7ef-490b-9cad-0ff9486633e1"
      }
    },
    "owner": "jyrwa-parkingease"
  }
}
